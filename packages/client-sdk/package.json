{
  "name": "@atoll/client-sdk",
  "version": "1.0.0",
  "description": "Atoll REST API client SDK",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "transpile": "tsc",
    "build": "rollup -c --bundleConfigAsCjs --verbose",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "watch": "rollup -cw --bundleConfigAsCjs",
    "clean": "npx --no-install rimraf dist",
    "test": "npm run test:tsc && npm run test:unit",
    "test:unit": "jest --coverage",
    "test:tsc": "tsc --noEmit --project tsconfig.json",
    "coverage-report": "open ./coverage/lcov-report/index.html"
  },
  "author": {
    "name": "Kevin Berry",
    "email": "41717340+51ngul4r1ty@users.noreply.github.com"
  },
  "license": "MIT",
  "peerDependencies": {
    "axios": "0.27.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@babel/preset-env": "7.17.10",
    "@babel/preset-typescript": "7.16.7",
    "@types/jest": "27.5.0",
    "autoprefixer": "10.4.5",
    "axios": "0.27.2",
    "jest-cli": "28.1.0",
    "jest-environment-jsdom": "28.1.0"
  },
  "dependencies": {
    "@atoll/api-types": "1.0.0",
    "@atoll/rest-fetch": "1.1.0",
    "@atoll/rich-types": "0.1.1",
    "http-status-codes": "2.2.0"
  }
}
