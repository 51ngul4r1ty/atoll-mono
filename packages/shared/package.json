{
  "name": "@atoll/shared",
  "version": "0.66.0",
  "description": "Atoll shared code that is common to electron desktop client and web app",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "prebuild": "echo Node Version && node --version",
    "build": "rollup -c",
    "clean": "npm run clean:dist",
    "clean:dist": "rimraf dist",
    "clean:node-modules": "rimraf node_modules",
    "gen:react-svg": "deno run --allow-read --allow-write --unstable ../scripts/build-svg-component/index.ts",
    "open:coverage-report": "npx --no-install opener ./coverage/lcov-report/index.html",
    "prepublishOnly": "npm run clean && npm run build && npm run test:unit",
    "publish-alpha": "npm publish --tag alpha",
    "report:coverage": "npm run test:unit && npm run open:coverage-report",
    "storybook": "npm run storybook:build && start-storybook -p 6006",
    "storybook:build": "npm run storybook:build-lib && npm run storybook:build-html",
    "storybook:build-lib": "npm run storybook:build-ts && npm run storybook:css-to-lib",
    "storybook:build-html": "npx --no-install ts-node  --project ./scripts/tsconfig.json ./scripts/build-storybook-html",
    "storybook:build-ts": "npx --no-install tsc -p tsconfig.storybook.json",
    "storybook:css-to-lib": "npx --no-install cpx \"./dist/index.cjs.css\" ./build/storybook",
    "test": "npm run test:tsc && npm run test:unit",
    "test:unit": "jest --env=jsdom --coverage",
    "test:tsc": "tsc --noEmit --project tsconfig.json"
  },
  "author": {
    "name": "Kevin Berry",
    "email": "41717340+51ngul4r1ty@users.noreply.github.com"
  },
  "license": "MIT",
  "dependencies": {
    "@flopflip/react-broadcast": "10.1.11",
    "axios": "0.21.2",
    "connected-react-router": "6.8.0",
    "history": "4.10.1",
    "http-status-codes": "2.2.0",
    "i18next": "19.0.0",
    "immer": "9.0.6",
    "query-string": "7.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "5.2.1",
    "react-i18next": "11.2.1",
    "react-markdown": "5.0.3",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "url-parse": "1.5.10",
    "websocket": "1.0.31"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-links": "6.5.12",
    "@storybook/addons": "6.5.12",
    "@storybook/cli": "6.5.12",
    "@storybook/react": "6.5.12",
    "@testing-library/react": "10.0.4",
    "@types/history": "4.7.5",
    "@types/i18next": "13.0.0",
    "@types/node": "13.13.4",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.5",
    "@types/react-helmet": "6.1.0",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.5",
    "@types/redux": "3.6.0",
    "@types/redux-mock-store": "1.0.2",
    "@types/url-parse": "1.4.8",
    "@types/websocket": "1.0.0",
    "autoprefixer": "10.4.11",
    "bs-logger": "0.2.6",
    "copy-webpack-plugin": "5.1.1",
    "cpx": "1.5.0",
    "launchdarkly-js-client-sdk": "2.22.1",
    "lodash.memoize": "4.1.2",
    "make-error": "1.3.6",
    "opener": "1.5.2",
    "redux-mock-store": "1.5.4",
    "storybook-addon-root-attribute": "1.0.2",
    "storybook-dark-mode": "1.1.2",
    "ts-node": "10.9.1"
  }
}
